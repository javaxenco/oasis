{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "oasis-garden-care",
  "buildCommand": "npm run build:web:prod",
  "outputDirectory": "dist/apps/web/browser",
  "framework": null,
  "installCommand": "npm install",
  "devCommand": "npm run start:web",
  "regions": ["iad1"],
  "public": false,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|jpg|jpeg|png|gif|ico|svg|woff|woff2|ttf|eot)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
